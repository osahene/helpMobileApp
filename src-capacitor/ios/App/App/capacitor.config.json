{
	"appId": "org.capacitor.helpmobile.app",
	"appName": "Help Mobile App",
	"webDir": "www",
	"server": {
		"androidScheme": "https",
		"url": "http://172.22.96.1:9500",
		"cleartext": true
	},
	"plugins": {
		"BackgroundRunner": {
			"label": "com.example.background.task",
			"src": "background.js",
			"event": "volumeButtonPressed",
			"repeat": true,
			"interval": 15,
			"autoStart": true
		},
		"SplashScreen": {
			"launchShowDuration": 5,
			"launchAutoHide": true,
			"launchFadeOutDuration": 5,
			"backgroundColor": "#b32424",
			"androidSplashResourceName": "splash",
			"androidScaleType": "CENTER_CROP",
			"showSpinner": false,
			"androidSpinnerStyle": "large",
			"iosSpinnerStyle": "small",
			"spinnerColor": "#999999",
			"splashFullScreen": false,
			"splashImmersive": false,
			"layoutName": "launch_screen",
			"useDialog": false
		},
		"Google": {
			"clientId": "972387283638-ad9c1rcda4sc1ki137f80u87po7f07l5.apps.googleusercontent.com",
			"scopes": [
				"profile",
				"email"
			]
		}
	},
	"android": {
		"allowMixedHttp": true,
		"foregroundService": {
			"notificationTitle": "Emergency Alert Service",
			"notificationBody": "Monitoring for emergency triggers",
			"notificationColor": "#FF0000"
		}
	},
	"packageClassList": [
		"AppPlugin",
		"GeolocationPlugin",
		"HapticsPlugin",
		"SocialLoginPlugin"
	]
}
